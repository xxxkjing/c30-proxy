{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "headers": {
          "Cache-Control": "no-cache, no-store, max-age=0, must-revalidate"
        },
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/proxy?path=$1"
      }
    ]
  }
  